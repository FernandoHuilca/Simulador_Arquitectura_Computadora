<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="667.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BusinessLogic.Controllers.LoginController">
   <children>
      <ImageView fitHeight="680.0" fitWidth="365.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Admin%20or%20user.png" />
         </image>
      </ImageView>
      <Button fx:id="botonAdmin" layoutX="131.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleButtonActionAdminLogin" prefHeight="45.0" prefWidth="102.0" style="-fx-background-color: #8881F5;" text="Admin" textFill="WHITE">
         <font>
            <Font name="Bookman Old Style Bold Italic" size="14.0" />
         </font></Button>
      <Button fx:id="botonUser" layoutX="131.0" layoutY="422.0" mnemonicParsing="false" onAction="#handleButtonActionUserLogin" prefHeight="45.0" prefWidth="102.0" style="-fx-background-color: #8881F5;" text="Usuario" textFill="WHITE">
         <font>
            <Font name="Bookman Old Style Bold Italic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
